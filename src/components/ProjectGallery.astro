---
interface Props {
  class?: string
  images: Array<Record<'src' | 'alt', string>>
}
const { class: customClass, images = [] } = Astro.props
---

<figure class='flex gap-10'>
  {
    images.map((image) => (
      <div class='h-60 w-full max-w-[420px] shrink-0 rounded-md bg-[#272729]' />
    ))
  }
</figure>
